{
  "name": "vscode-logreader-extension",
  "displayName": "Log Reader Extension",
  "description": "A VS Code extension to read and retrieve log files from a local MCP server.",
  "version": "0.1.0",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "onCommand:logreader.getLogfiles"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "logreader.getLogfiles",
        "title": "Get Log Files"
      },
      {
        "command": "logreader.clearOutput",
        "title": "Clear Log Reader Output"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile"
  },
  "devDependencies": {
    "@types/node": "^16.18.126",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}
